<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>AxolotlTank</defName>
    <label>AxolotlTank</label>
    <thingClass>DuneTleilax.Building_GeneChamber</thingClass>
    <designationCategory>Biotech</designationCategory>
    <tickerType>Normal</tickerType>
    <graphicData>
      <texPath>AxolotlTank</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>

    <size>(1,2)</size>
    <researchPrerequisites>
      <li>Tleilaxu_Ways</li>
    </researchPrerequisites>
    <comps>
      <li Class="CompProperties_Refuelable">
        <fuelFilter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </fuelFilter>
        <fuelCapacity>200</fuelCapacity>
        <fuelConsumptionRate>0.05</fuelConsumptionRate>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
      </li>

      <li Class="DuneTleilax.CompGeneChamber"/>
    </comps>

    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(0, 0, -1)</interactionCellOffset>
  </ThingDef>
</Defs>
